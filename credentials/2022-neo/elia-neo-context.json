{
  "@context": {
    "@version": 1.1,
    "@protected": true,
    "elia": "https://vc-context.elia.be/2022/v1/",
    "saref4ener": "https://saref.etsi.org/saref4ener/",

    "PowerProducingDeviceAssessmentCredential": "elia:PowerProductinDeviceAssessmentCredential",
    "AssessedPowerProducingDevice": {
      "@id": "elia:AssessedPowerProducingDevice",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "powerSource": "saref4ener:powerSource",
        "nominalVoltageProduction": "elia:nominalVoltageProduction",
        "maximumVoltageProduction": "elia:maximumVoltageProduction",
        "cutoffVoltageProduction": "elia:cutoffVoltageProduction",
        "nominalCurrentProduction": "elia:nominalCurrentProduction",
        "maximumCurrentProduction": "elia:maximumCurrentProduction",
        "cutoffCurrentProduction": "elia:cutoffVoltageProduction",
        "cutoffFrequencyProduction": "elia:cutoffFrequencyProduction",
        "maximumActivePowerProduction": "elia:maximumActivePowerProduction",
        "maximumReactivePowerProduction": "elia:maximumReactivePowerProduction",
        "nominalActivePowerProduction": "elia:nominalActivePowerProduction",
        "nominalReactivePowerProduction": "elia:nominalReactivePowerProduction",
        "marginalProductionCosts": "elia:marginalProductionCosts"
      }
    },
    
    "LoadControlableDeviceCredential": "elia:LoadControlableDeviceCredential",
    "LoadControlableDevice": {
      "@id": "elia:LoadControlableDevice",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "isDirectReceiverOfLoadControlEvent": "elia:isDirectReceiverOfLoadControlEvent",
        "supportsLoadControlEvent": "elia:supportsLoadControlEvent",
        "isRemoteControlable": "elia:isRemoteControlable",
        "minimumDelayToRespondToActivation": "elia:minimumDelayToRespondToActivation",
        "powerControlTheoretical": "elia:powerControlTheoretical",
        "powerRampupProfileTypeAvailable": "elia:powerRampupProfileTypeAvailable",
        "powerRampupProfileDurationToRaise50Percent": "elia:powerRampupProfileDurationToRaise50Percent",
        "isPowerRampupProfileDurationToRaise50PercentConfigurable": "elia:isPowerRampupProfileDurationToRaise50PercentConfigurable",
        "powerRampupProfileDurationToRaise100Percent": "elia:powerRampupProfileDurationToRaise100Percent",
        "isPowerRampupProfileDurationToRaise100PercentConfigurable": "elia:isPowerRampupProfileDurationToRaise100PercentConfigurable"
      }
    },

    "DeviceLocationCredential": "elia:DeviceLocationCredential",
    "DeviceLocation": {
      "@id": "elia:DeviceLocation",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "street": "elia:street",
        "number": "elia:number",
        "postalCode": "elia:postalCode",
        "city": "elia:city",
        "region": "elia:region",
        "state": "elia:state"
      }
    },

    "EnergyLocationCredential": "elia:DeviceLocationCredential",
    "EnergyLocation": {
      "@id": "elia:EnergyLocation",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "localIdentificationMethodOfAccessPoint": "elia:localIdentificationMethodOfAccessPoint",
        "number": "elia:number",
        "postalCode": "elia:postalCode",
        "city": "elia:city",
        "region": "elia:region",
        "state": "elia:state"
      }
    }, 

    "SteeringListCredential": "elia:SteeringListCredential",
    "SteeringList": {
      "@id": "elia:SteeringList",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "connectivityStandards": "elia:connectivityStandards"
      }
    }
  }
}