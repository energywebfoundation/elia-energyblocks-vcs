{
  "@context": {
    "@version": 1.1,
    "@protected": true,
    "elia": "https://vc-context.elia.be/2022/v1/",
    "saref4ener": "https://saref.etsi.org/saref4ener/",

    "DeviceInfoCredential": "elia:DeviceInfoCredential",
    "Device": {
      "@id": "saref4ener:Device",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "brandName": "saref4ener:brandName",
        "deviceCode": "saref4ener:deviceCode",
        "deviceName": "saref4ener:deviceName",
        "manufacturerDescription": "saref4ener:manufacturerDescription",
        "manufacturerLabel": "saref4ener:manufacturerLabel",
        "hardwareRevision": "saref4ener:hardwareRevision"
      }
    },
    
    "PowerConsumingDeviceAssessmentCredential": "elia:PowerConsumingDeviceAssessmentCredential",
    "AssessedPowerConsumingDevice": {
      "@id": "elia:AssessedPowerConsumingDevice",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "powerSource": "saref4ener:powerSource",
        "nominalVoltageConsumption": "elia:nominalVoltageConsumption",
        "maximumVoltageConsumption": "elia:maximumVoltageConsumption",
        "cutoffVoltageConsumption": "elia:cutoffVoltageConsumption",
        "nominalCurrentConsumption": "elia:nominalCurrentConsumption",
        "maximumCurrentConsumption": "elia:maximumCurrentConsumption",
        "cutoffCurrentConsumption": "elia:cutoffVoltageConsumption",
        "cutoffFrequencyConsumption": "elia:cutoffFrequencyConsumption",
        "maximumActivePowerConsumption": "elia:maximumActivePowerConsumption",
        "maximumReactivePowerConsumption": "elia:maximumReactivePowerConsumption",
        "nominalActivePowerConsumption": "elia:nominalActivePowerConsumption",
        "nominalReactivePowerConsumption": "elia:nominalReactivePowerConsumption",
        "marginalConsumptionCosts": "elia:marginalConsumptionCosts"
      }
    },

    "EnergyStorageDeviceCredential": "elia:EnergyStorageDeviceCredential",
    "EnergyStorageDevice": {
      "@id": "elia:EnergyStorageDevice",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "capacityTotalTheoretical": "elia:capacityTotalTheoretical",
        "depthOfDischarge": "elia:depthOfDischarge",
        "annualizedWarrantyCapacity": "elia:annualizedWarrantyCapacity"
      }
    },

    "MeteringCapableDeviceCredential": "elia:MeteringCapableDeviceCredential",
    "MeteringCapableDevice": {
      "@id": "elia:MeteringCapableDevice",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "capableOfMetering": "elia:capableOfMetering",
        "typeOfMetering": "elia:typeOfMetering",
        "precisionClassOfMeter": "elia:precisionClassOfMeter",
        "areDataAggregated": "elia:areDataAggregated",
        "numSecondsOfAggregation": "elia:numSecondsOfAggregation"
      }
    },

    "LoadControlableDeviceCredential": "elia:LoadControlableDeviceCredential",
    "LoadControlableDevice": {
      "@id": "elia:LoadControlableDevice",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "isDirectReceiverOfLoadControlEvent": "elia:isDirectReceiverOfLoadControlEvent",
        "supportsLoadControlEvent": "elia:supportsLoadControlEvent",
        "isRemoteControlable": "elia:isRemoteControlable",
        "minimumDelayToRespondToActivation": "elia:minimumDelayToRespondToActivation",
        "powerControlTheoretical": "elia:powerControlTheoretical",
        "powerRampupProfileTypeAvailable": "elia:powerRampupProfileTypeAvailable",
        "powerRampupProfileDurationToRaise50Percent": "elia:powerRampupProfileDurationToRaise50Percent",
        "isPowerRampupProfileDurationToRaise50PercentConfigurable": "elia:isPowerRampupProfileDurationToRaise50PercentConfigurable",
        "powerRampupProfileDurationToRaise100Percent": "elia:powerRampupProfileDurationToRaise100Percent",
        "isPowerRampupProfileDurationToRaise100PercentConfigurable": "elia:isPowerRampupProfileDurationToRaise100PercentConfigurable"
      }
    },

    "DeviceLocationCredential": "elia:DeviceLocationCredential",
    "DeviceLocation": {
      "@id": "elia:DeviceLocation",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "street": "elia:street",
        "number": "elia:number",
        "postalCode": "elia:postalCode",
        "city": "elia:city",
        "region": "elia:region",
        "state": "elia:state"
      }
    },

    "EnergyLocationCredential": "elia:DeviceLocationCredential",
    "EnergyLocation": {
      "@id": "elia:EnergyLocation",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "localIdentificationMethodOfAccessPoint": "elia:localIdentificationMethodOfAccessPoint",
        "number": "elia:number",
        "postalCode": "elia:postalCode",
        "city": "elia:city",
        "region": "elia:region",
        "state": "elia:state"
      }
    }, 

    "SteeingListCredential": "elia:SteeringListCredential",
    "SteeringList": {
      "@id": "elia:SteeringList",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "connectivityStandards": "elia:connectivityStandards"
      }
    }
  }
}