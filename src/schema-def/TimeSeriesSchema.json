{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$id": "https://elia.be/timeseriesschema.json",
    "type": "object",
    "properties": {
      "id": {
        "type": "string"
      },
      "reading": {
        "type": "object",
        "properties": {
          "timeStart": {
            "type": "string"
          },
          "timeEnd": {
            "type": "string"
          },
          "energy": {
            "type": "integer",
            "minimum": 0,
            "exclusiveMinimum": true
          }
        },
        "required": [
          "timeStart",
          "timeEnd",
          "energy"
        ]
      }
    },
    "required": [
      "id",
      "reading"
    ]
  }